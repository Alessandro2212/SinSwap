@model IList<AboutUsModel>

@if (Model.Count > 0)
{
    <div class="container aboutUsContainer">
        <div class="row">
            <div class="col">
                <div class="container">
                    <h2>
                        About <strong>sinswap</strong>
                    </h2>
                    <div class="row">
                        <div class="col-12">
                            <div class="content">
                                @foreach (var item in Model)
                                {
                                    <h3>@item.Title</h3>
                                    <p>
                                        @item.Content
                                    </p>
                                }
                                <a href="@Url.RouteUrl("AboutUs")" class="btn btn-light">Read more about <strong>us</strong></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}